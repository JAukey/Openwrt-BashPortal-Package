config bashportal 'base'		
    option enabled '0'
	option hostip '192.168.1.1'
	option httpport '2561'
    option portalpath '/etc/bashportal/html'
    option luafile '/etc/bashportal/html/lua/portal.lua'
    option ngxconf '/etc/nginx/nginx.conf'

config bashportal 'interface'
	list interface 'br-lan'
